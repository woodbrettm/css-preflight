{
  "$schema": "https://json.schemastore.org/package",
  "name": "css-preflight",
  "version": "1.1.1",
  "type": "module",
  "license": "MIT",
  "description": "CSS browser reset based on Tailwind's preflight, with optional enhancements.",
  "author": {
    "name": "Brett Wood",
    "url": "https://github.com/woodbrettm"
  },
  "bugs": "https://github.com/woodbrettm/css-preflight/issues",
  "homepage": "https://github.com/woodbrettm/css-preflight",
  "repository": {
    "type": "git",
    "url": "https://github.com/woodbrettm/css-preflight"
  },
  "keywords": [
    "css",
    "preflight",
    "normalize",
    "reset",
    "normalizer",
    "browser"
  ],
  "main": "./styles/preflight.css",
  "style": "./styles/preflight.css",
  "exports": {
    ".": "./styles/preflight.css",
    "./preflight.css": "./styles/preflight.css",
    "./font-mono.css": "./styles/font-mono.css",
    "./font-sans-serif.css": "./styles/font-sans-serif.css",
    "./font-serif.css": "./styles/font-serif.css",
    "./mono-elements.css": "./styles/mono-elements.css",
    "./rem-same-px.css": "./styles/rem-same-px.css",
    "./smooth-scroll.css": "./styles/smooth-scroll.css",
    "./text-wrap-balance.css": "./styles/text-wrap-balance.css"
  },
  "files": [
    "LICENSE.md",
    "LICENSE_TAILWIND.md",
    "styles",
    "package.json",
    "README.md"
  ],
  "devDependencies": {
    "@changesets/cli": "2.29.5",
    "prettier": "3.6.2",
    "vite": "7.0.4",
    "eslint": "9.31.0",
    "@eslint/css": "0.10.0"
  },
    "scripts": {
    "changeset:version": "changeset version",
    "changeset:add": "changeset",
    "changeset:release": "changeset publish",
    "dev": "vite dev --config \"./playground/vite.config.js\"",
    "lint:css": "eslint \"./styles/**\""
  }
}
